<% if notice %>
<div class="alert-success">
  <p id="notice"><%= notice %></p>
</div>
<% end %>
<!-- <h4>Список администраторов: </h4> -->
<h4><%= I18n.t('admin.info.index.title')%></h4>
<div class="table-responsive">
 <table class="table table-striped">
		
    <tr>
    	<th>N n/n</th>
    	<th>First name </th>
    	<th>Last name </th>
    	<th>Email </th>
    	<th>Birthdate </th>
    	<th></th>
    </tr>
   
  
    <% @users.each do |user| %>
    <tr>
		<td><%= user.id %></td>
		<td><%= user.first_name%></td>
		<td><%= user.last_name%></td>
		<td><%= user.email%></td>
		<td><%= user.birthdate%></td>
		
		<td><%= link_to "Редактирование", edit_admin_info_path(user) %> </td>
								
    </tr>		  		
   <% end %>
 
</table>
</div>
